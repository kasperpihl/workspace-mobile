{
  "presets": ["module:metro-react-native-babel-preset", "@babel/preset-flow"],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "src": "./src",
          "swipes-core-js": "@swipesapp/core/dist",
          "assets": "./assets"
        }
      }
    ]
  ]
}
